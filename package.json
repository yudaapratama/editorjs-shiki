{
  "name": "@nuxtizen/editorjs-shiki",
  "version": "1.0.6",
  "keywords": [
    "code",
    "editor.js",
    "editorjs",
    "shiki"
  ],
  "description": "Code tool using shiki for editorjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yudaapratama/editorjs-shiki.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/code.umd.js",
  "module": "./dist/code.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/code.mjs",
      "require": "./dist/code.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint",
    "lint:errors": "eslint --quiet",
    "lint:fix": "eslint --fix",
    "publish": "vite build && npm publish --access public"
  },
  "author": "Yuda Pratama <yudhaprathamaa@gmail.com>",
  "devDependencies": {
    "@editorjs/editorjs": "^2.30.8",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^9.9.1",
    "shiki": "^2.4.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.4.3",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-dts": "^3.9.1"
  },
  "bugs": {
    "url": "https://github.com/yudaapratama/editorjs-shiki/issues"
  },
  "homepage": "https://github.com/yudaapratama/editorjs-shiki#readme"
}
